# mystats: 一个描述性统计量计算函数
mystats <- function(x, parametric=T, print=F){
if (parametric){
center <- mean(x); spread <- sd(x)
} else {
center <- median(x); spread <- mad(x)
}
if (print & parametric) {
cat("Meidan=", center, "\n", "MAD=", spread, "\n")
}
result <- list(center=center, spread=spread)
return(result)
}
